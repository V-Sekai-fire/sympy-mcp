{
  "name": "SymPy MCP Development",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "extensions": [
        "JakeBecker.elixir-ls",
        "ms-vscode.vscode-json",
        "eamodio.gitlens",
        "saoudrizwan.claude-dev"
      ],
      "settings": {
        "elixirLS.fetchDeps": true,
        "editor.formatOnSave": true,
        "elixirLS.dialyzerEnabled": false
      }
    }
  },
  "remoteUser": "dev",
  "features": {
    "ghcr.io/devcontainers/features/git:1": {}
  },
  "forwardPorts": [],
  "remoteEnv": {
    "PATH": "/opt/elixir/bin:/home/dev/.local/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
  }
}
